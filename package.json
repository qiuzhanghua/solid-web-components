{
  "name": "@qiuzhanghua/solid-element",
  "version": "0.0.3",
  "description": "Use solidjs in web components",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
    "files": [
        "dist"
    ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "dts": "tsc --emitDeclarationOnly --declaration --allowJs --checkJs --jsx preserve --target esnext --moduleResolution node --outDir dist/types src/index.ts",
    "build-all": "vite build && tsc",
    "serve": "vite preview",
    "lint": "eslint src --ext .ts --ext .tsx",
    "format": "prettier-eslint \"src/**/*.ts\" \"src/**/*.tsx\" --write",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "clean": "rimraf dist"
  },
  "license": "MIT",
  "devDependencies": {
    "@solidjs/testing-library": "^0.7.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/node": "^20.1.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@vitest/coverage-c8": "^0.31.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-solid": "^0.12.1",
    "jsdom": "^22.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-eslint-cli": "^7.1.0",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "solid-element": "^1.7.0",
    "solid-js": "^1.7.5"
  }
}
